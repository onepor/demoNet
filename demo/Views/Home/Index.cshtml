@{
    ViewBag.Title = "Home Page";
}
<script src="~/Scripts/jquery-3.3.1.min.js"></script>
<script type="text/javascript">
    function cc() {
        $.ajax({
            type: "post",
            url: "/Home/UploadUserAdd",
            cache: false,
            data: { uuid: $("#hfSearchUUID").val() },
            dataType: "json",
            success: function (data) {
                //console.log(data);
                $("#aa").html(data);
            }
        });
    }
</script>
<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="Home/retxls" class="btn btn-primary btn-lg">导出 &raquo;</a></p>
    @*<input class="file_input" type="file" name="file" id="file" onchange="$('#btnUpload').click()" />上传文件*@
    <p><a onclick="cc()" class="btn btn-primary btn-lg">显示 &raquo;</a></p>
    @using (Html.BeginForm("UploadUserAdd1", "UserManager", FormMethod.Post, new { id = "form1", enctype = "multipart/form-data", target = "rfFrame" }))
    {
        <ul>
            <li>

                <label class="xzwj">选择文件：</label>
                <input disabled="disabled" type="text" class="file_in" />
